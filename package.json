{
  "name": "new-koa-jwt",
  "version": "1.0.0",
  "description": "Koa.js middleware for validates a jwt",
  "scripts": {
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint:fix": "tslint --fix -c tslint.json -p tsconfig.json",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "build": "tsc"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "author": "rs-hub",
  "license": "ISC",
  "nodemonConfig": {
    "ext": "ts,tsx,yml",
    "watch": [
      "src/**/*",
      "api-docs/"
    ],
    "exec": "node --inspect -r ts-node/register ./src/example.ts"
  },
  "readmeFilename": "README.md",
  "husky": {
    "hooks": {
      "pre-commit": "npm run tslint:fix && npm run tslint"
    }
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "husky": "^3.0.9",
    "mocha": "^6.2.1",
    "@types/koa-compose": "^3.2.4",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.3.5",
    "@types/koa": "^2.0.51",
    "bluebird": "^3.7.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.10.0"
  }
}
